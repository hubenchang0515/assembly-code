{
    "name": "assembly-code",
    "displayName": "Assembly Code",
    "description": "NASM assembly language syntax highlight",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "asm",
            "aliases": ["Assembly", "asm"],
            "extensions": [".s",".asm"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "asm",
            "scopeName": "source.s",
            "path": "./syntaxes/asm.tmLanguage.json"
        }]
    }
}